<% layout("/layouts/boilerplate")%>

  <body>
    <h3>all listings</h3>
    <div class="row row-cols-lg-3 row-cols-md-2 row-cols-sm-1 ">
      <%for(let listing of allListings){%>
        <a href="/listings/<%= listing._id%>" class="l-link">
          <div class="card col">
            <div class="card-img-overlay">abcd</div>
            <img src="<%= listing.Image%>" class="card-img-top" alt="listing_image">
            <div class="card-body">
              <p class="card-text">
                <%=listing.title%> <br>
                  &#8377; <%=listing.price%>

              </p>
            </div>

          </div>
        </a>
        <%}%>
    </div>